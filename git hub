PROGRAM 10

mkdir <folder-name>
cd <folder-name>
git init
nano file.txt
git add file.txt
git commit -m"commit message"
git log --oneline
git show <commit-id>
git log --author="author-name" --since="starting-date" --until="ending-date"    

PROGRAM 11

mkdir <folder-name>
cd <folder-name>
git init
nano file.txt
git add file.txt
git commit -m "1st commit"
nano file.txt
git add file.txt
git commit -m "2nd commit"
nano file.txt
git add file.txt
git commit -m "3rd commit"
nano file.txt
git add file.txt
git commit -m "4th commit"
nano file.txt
git add file.txt
git commit -m "5th commit"
git log -n 5



PROGRAM 12

mkdir <folder-name>
cd <folder-name>
git init
nano file.txt
git add file.txt
git commit -m"commit message"
git log --oneline
git revert <commit-id>