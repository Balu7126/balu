git config --global user.name "abhishekawaradi"
git config --global --list

PROGRAM 01

mkdir <folder-name>             
cd <folder-name>                 
git init                        
nano file.txt                  
git add file.txt                 
git commit -m "added via commit" 

PROGRAM 02

mkdir <folder-name>
cd <folder-name>
git init
git checkout -b feature-branch  
nano file.txt
git add file.txt
git commit -m "added via commit"
git checkout master            
git merge feature-branch        

PROGRAM 03

mkdir <folder-name>
cd <folder-name>
git init
git checkout -b feature-branch
nano file.txt
git add file.txt
git stash push --staged
git checkout master
git stash apply 
git remote add origin <repo-url>                    

PROGRAM 04

git clone <repository_url> 

mkdir <folder-name2>
cd <folder-name2>
git init
nano file.txt
git add file.txt
git commit -m "commit message"  
git remote add origin https://github.com/username/repository.git
git push origin master
   

PROGRAM 05

git clone <repository_url>
cd <repo-name>
nano file.txt
git add file.txt
git commit -m "commit messsage"
git checkout master
git fetch origin                       
git rebase origin/master                
         

PROGRAM 06

mkdir <folder-name>
cd <folder-name>
git init
git checkout -b feature-branch
nano file.txt
git add file.txt
git commit -m "added via commit"
git checkout master
git merge feature-branch -m "Merge feature-branch into master"

PROGRAM 07

mkdir <folder-name>
cd <folder-name>
git init
nano file.txt
